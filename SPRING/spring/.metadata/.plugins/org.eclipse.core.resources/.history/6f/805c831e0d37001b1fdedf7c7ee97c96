//$(선택자).이벤트함수(코드)
//$(document).ready(function() {})
$(function() {
	$.ajax({
		type:"post", 
		url:"background.do", 
		success:function(result) {	//result는 background.do에서 return된 데이터
			console.log("test:"+result);
			$("#result").html( "상품명:"+result.name +", 가격:"+result.price);
		}
	});
});